html, body
  background black
  margin 0
  padding 0
  height 100%
  width 100%

#content
  display none
  text-align center

#circle circle
  fill none
  pointer-events all

#spin
  height 100%
  width 100%

.group path
  fill-opacity .5

path.chord
  stroke #000
  stroke-width .25px

#circle:hover path.fade
  display none
